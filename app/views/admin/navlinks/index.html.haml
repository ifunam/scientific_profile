%h3=t('.title')
%div{ :id=>'collection', :class => 'span-24 section_left last'}
  %div{:class => 'span-24 row_title last'}
    %div{:class => 'span-17'}= t '.title'
    %div{:class => 'span-3'}= t '.move'
    %div{:class => 'span-3'}= t '.actions'
  = render :partial => @navlinks
%div{:class => 'span-24 last'}
  = will_paginate @navlinks

%div{:class => 'span-24 ui-buttons last'}
  = button_link_to t(:new), new_admin_navlink_path