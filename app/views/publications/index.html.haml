%h3=t('.title')
%div{ :id=>'collection', :class => 'span-24 section_left last'}
  %div{:class => 'span-24 row_title last'}
    %div{:class => 'span-18'}= t '.publication'
    %div{:class => 'span-2'}= t '.file'
    %div{:class => 'span-3'}= t '.actions'
  = render :partial => @publications
%div{:class => 'span-24 last'}
  = will_paginate @publications

%div{:class => 'span-24 ui-buttons last'}
  = button_link_to t(:new), new_publication_path